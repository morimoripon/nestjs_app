    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prisma:generate": "prisma generate",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "tsc": "tsc --noEmit",
    "test": "DATABASE_URL=postgresql://postgres:password@localhost:5435/morimoripon?schema=public jest",
    "test:watch": "DATABASE_URL=postgresql://postgres:password@localhost:5435/morimoripon?schema=public jest --watch",
    "test:cov": "DATABASE_URL=postgresql://postgres:password@localhost:5435/morimoripon?schema=public jest --coverage",
    "test:debug": "DATABASE_URL=postgresql://postgres:password@localhost:5435/morimoripon?schema=public node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "DATABASE_URL=postgresql://postgres:password@localhost:5435/morimoripon?schema=public jest --config ./test/jest-e2e.json",
    "db:test:migrate": "DATABASE_URL=postgresql://postgres:password@localhost:5435/morimoripon?schema=public npx prisma migrate dev"
  },
